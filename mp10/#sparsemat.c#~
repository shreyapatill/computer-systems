#include "sparsemat.h"

#include <stdio.h>
#include <stdlib.h>





sp_tuples * load_tuples(char* input_file)
{
  int row, col;
  double val;

  FILE *file = fopen(input_file, "r");
  sp_tuples *matrix = malloc(sizeof(sp_tuples));
  fscanf( file, "%d %d\n", &(matrix->m), &(matrix->n) );
  
  while( feof(file) == 0 ){
    fscanf( file, "%d %d %d\m", &row, &col, &val )
      set_tuples( matrix, row, col, val );
  }
    return NULL;
    
}



